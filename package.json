{
  "name": "gridvue2",
  "version": "0.1.6",
  "private": false,
  "main": "./dist/gridvue2.common.js",
  "module": "./dist/gridvue2.esm.js",
  "unpkg": "./dist/gridvue2.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/imanfattahi/gridvue2.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name gridvue2 ./src/main.js"
  },
  "keywords": [
    "gridview",
    "gridvue",
    "gridvue2",
    "table",
    "data-table",
    "ajax-table",
    "searchable"
  ],
  "description": "GridVue2 is powerfull gridview for vuejs",
  "author": "Iman Fattahi <imanfava@gmail.com>",
  "dependencies": {
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "license": "MIT"
}
